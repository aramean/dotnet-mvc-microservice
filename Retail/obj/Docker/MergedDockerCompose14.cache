networks:
  gunnebo-net:
    driver: bridge
services:
  gunnebo:
    build:
      context: /Users/josef/Projects/Gunnebo/Retail
      dockerfile: Gunnebo/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Gunnebo
      target: base
    depends_on:
    - gunnebo-db
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTPS_PORT: '44396'
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: gunnebo:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "bin/Debug/netcoreapp3.0/Gunnebo.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    links:
    - gunnebo-db
    networks:
      gunnebo-net: {}
    ports:
    - published: 55861
      target: 80
    - published: 44396
      target: 443
    tty: true
    volumes:
    - /Developer/Gunnebo/Retail/Gunnebo:/app:rw
    - /Users/josef/vsdbg/vs2017u5:/remote_debugger:rw
    - /Users/josef/.aspnet/https:/root/.aspnet/https:ro
    - /Users/josef/.microsoft/usersecrets:/root/.microsoft/usersecrets:ro
    - /usr/local/share/dotnet/sdk/NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - /Users/josef/.nuget/packages:/root/.nuget/packages:ro
    - /Developer/Gunnebo/Retail:/src:rw
  gunnebo-db:
    container_name: gunnebo-mssql
    environment:
      ACCEPT_EULA: "Y"
      MSSQL_PID: Developer
      SA_PASSWORD: yourStrong(!)Password
    hostname: gunnebo-dbhost
    image: mcr.microsoft.com/mssql/server:2017-latest-ubuntu
    networks:
      gunnebo-net: {}
    ports:
    - published: 1433
      target: 1433
    restart: always
version: '3.4'